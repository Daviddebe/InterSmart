<Window x:Class="InterSmartV1._1.Menu.Options"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Options" Height="300" Width="300">
    
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <CheckBox x:Name="chbxTweetAnoniem" Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="2" Checked="chbxTweetAnoniem_Checked" Unchecked="chbxTweetAnoniem_Unchecked_1" >Tweet Anoniem</CheckBox>
        <Button Grid.Column="4" Grid.Row="4" Click="Button_Click_1">Back</Button>
    </Grid>
</Window>
